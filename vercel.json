{
  "version": 2,
  "builds": [
    {
      "src": "dist/frontend/index.html",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "dist/frontend/index.html"
    }
  ],
  "buildCommand": "npm run build:prod",
  "installCommand": "npm install",
  "outputDirectory": "dist/frontend"
}